                    li.nav-item
                        button.nav-link
                            img.colorsvg(src="img/guardado.svg" width="60px" height="24px")
                            | Guardado
                    li.nav-item
                        button.nav-link
                            img.colorsvg(src="img/lista.svg" width="60px" height="24px")
                            | Listas
                    li.nav-item
                        button.nav-link Perfil
                    li.nav-item
                        button.nav-link
                            img.colorsvg(src="img/opciones.svg" width="60px" height="28px")
